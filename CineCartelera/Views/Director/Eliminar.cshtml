@model CineCartelera.Models.Director

@{
    ViewData["Title"] = "Eliminar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Editar</h1>

<form method="post" asp-action="Eliminar">
    <input asp-for="Id" hidden />
    <div class="border mt-5 p-3">

        <div asp-validation-summary="ModelOnly" class="text-danger">


        </div>
        <div class="form-group row">
            <h2 class="text-light pl-3 text-center">Eliminar este Director</h2>
        </div>
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-md-4">
                <label asp-for="Nombre" class="form-label">Género</label>
               <input asp-for="Nombre" disabled class="form-control" />
             


            </div>
        </div>

         

        <br />

        <div class=" d-grid gap-3 d-md-block text-center">
            <button type="submit" class="btn btn-outline-info">Eliminar</button>
            <a asp-controller="Director" asp-action="Index" class="btn btn-outline-success">Regresar</a>
    </div>
    </div>
    </div>


</form>

@section Scripts {
    @{
        <partial name="_ValidationScriptsPartial" />
    }

}
